{
  "name": "vitepress-theme-maple",
  "version": "1.0.4",
  "description": "A minimalist VitePress theme with a clean editorial feel",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./loader": {
      "types": "./dist/loader.d.ts",
      "import": "./dist/loader.js"
    },
    "./styles": "./dist/vitepress-theme-maple.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "vite build --watch"
  },
  "keywords": [
    "vitepress",
    "theme",
    "blog",
    "minimalist",
    "maple"
  ],
  "author": "huanjun",
  "license": "MIT",
  "peerDependencies": {
    "vitepress": "^1.0.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.7",
    "@vitejs/plugin-vue": "^6.0.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^3.2.2"
  },
  "dependencies": {
    "medium-zoom": "^1.1.0"
  }
}
